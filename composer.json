{
    "name": "sarahman/oauth-tokens-client",
    "description": "This package handles the oauth tokens with caching functionality.",
    "license": "MIT",
    "type": "library",
    "keywords": ["laravel", "library", "passport", "oauth", "tokens", "client", "cache"],
    "version": "1.1.0",
    "authors": [
        {
            "name": "Syed Abidur Rahman",
            "email": "aabid048@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sarahman/oauth-tokens-client/issues",
        "source": "https://github.com/sarahman/oauth-tokens-client"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "illuminate/cache": ">=4.0",
        "illuminate/support": ">=4.0",
        "psr/http-message": "^1.0",
        "sarahman/laravel-http-request-api-log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sarahman\\OauthTokensClient\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 0,
        "secure-http": false,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sarahman\\OauthTokensClient\\OauthTokensClientServiceProvider"
            ]
        }
    }
}
