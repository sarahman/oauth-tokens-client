{
    "name": "sarahman/oauth-tokens-client",
    "description": "This package handles the oauth tokens with caching functionality.",
    "keywords": ["http", "api", "request", "framework", "laravel", "lumen"],
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Syed Abidur Rahman",
            "email": "aabid048@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": ">=4.0",
        "illuminate/support": ">=4.0",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sarahman\\OauthTokensClient\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sarahman\\OauthTokensClient\\OauthTokensClientServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
